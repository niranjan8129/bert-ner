sudo: required 

language: python

python:
  - "3.6"      # current default Python on Travis CI
   
service:
 - docker


before_install:
 - docker build -t deploy/app -f ./Dockerfile.txt .

before_install: pip install docker-compose 

script:
 - docker run -p 5000:8000 deploy/app
 
deploy;
 provider: elasticbeanstalk
 region: "ap-south-1"
 app: "bertner"
 env: "Bertner-env"
 bucket_name: "elasticbeanstalk-ap-south-1-238543985800"
 bucket_path: "bertner"
 on:
   branch: master
 access_key_id: $AWS_ACCESS_KEY
 secret_access_key:
   secure: "$AWS_SECRET_KEY"